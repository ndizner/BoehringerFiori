{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Types",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Types/types-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/trainingbiUI5Module/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Types/UI5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Types/UI5Module\"}"
      },
      "envFile": "${workspaceFolder}/Types/UI5Module/.env1",
      "preLaunchTask": "Build Types"
    },
    {
      "name": "Run Formatter",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Formatter/formatter-approuter/node_modules/@sap/html5-repo-mock/index.js",
