      "preLaunchTask": "Build Formatter"
    },
    {
      "name": "Run i18n",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/i18n/i18n-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/trainingbiUI5Module/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/i18n/UI5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/i18n/UI5Module\"}"
